generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id          String   @id @default(uuid())
  title       String
  amount      Float
  type        String
  category    String
  date        String
  createdAt   DateTime @default(now())
}
